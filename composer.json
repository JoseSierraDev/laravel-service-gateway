{
  "name": "josesierradev/laravel-service-gateway",
  "description": "A Laravel package to generate Service and Gateway classes for clean architecture projects.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jose Sierra",
      "email": "jodasihe10@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "illuminate/support": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelServiceGateway\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelServiceGateway\\LaravelServiceGatewayServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
